<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FDE4F7"
    tools:context=".Fragment.MyPageFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/logoimageView"
                android:layout_width="411dp"
                android:layout_height="92dp"
                android:layout_gravity="center"
                android:layout_marginTop="40dp"
                tools:src="@drawable/logoimage" />

            <TextView
                android:id="@+id/Mypage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="16dp"
                android:fontFamily="@font/kcchanbit"
                android:text="MY PAGE"
                android:textColor="#FF07194B"
                android:textSize="34sp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:layout_marginStart="16dp"
                android:layout_marginBottom="5dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/customerName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/kcchanbit"
                    android:textColor="#FF07194B"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/yourcoloris"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/kcchanbit"
                    android:text="님의 퍼스널 컬러는"
                    android:textColor="#FF07194B"
                    android:textSize="16sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="67dp"
                android:layout_gravity="left"
                android:layout_marginStart="16dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/PersonalColorName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginBottom="10dp"
                    android:fontFamily="@font/kcchanbit"
                    android:textColor="#FF07194B"
                    android:textSize="34sp" />

                <TextView
                    android:id="@+id/itis"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="@font/kcchanbit"
                    android:text="입니다."
                    android:textColor="#FF07194B"
                    android:textSize="16sp" />
            </LinearLayout>

            <TextView
                android:id="@+id/recommendBase"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="16dp"
                android:fontFamily="@font/kcchanbit"
                android:text="추천 베이스"
                android:textColor="#FF07194B"
                android:textSize="16sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/baseimageView_1"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_weight="1" />

                <ImageView
                    android:id="@+id/baseimageView_2"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_gravity="center" />

                <ImageView
                    android:id="@+id/baseimageView_3"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_weight="1" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <!-- First LinearLayout -->
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="left">

                    <TextView
                        android:id="@+id/baseproductTextView_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/kcchanbit"
                        android:gravity="left"
                        />

                    <TextView
                        android:id="@+id/baseproductTextView_11"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/kcchanbit"
                        android:textColor="#483D8B"
                        android:gravity="left"
                        android:textStyle="bold"
                        />

                    <TextView
                        android:id="@+id/baseproductTextView_111"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/kcchanbit"
                        android:textSize="10dp"
                        android:textColor="#B0B0B0"
                        android:gravity="left"/>

                </LinearLayout>

                <!-- Second LinearLayout -->
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="left">

                    <TextView
                        android:id="@+id/baseproductTextView_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/kcchanbit"
                        android:gravity="left"/>

                    <TextView
                        android:id="@+id/baseproductTextView_22"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/kcchanbit"
                        android:textColor="#483D8B"
                        android:textStyle="bold"
                        android:gravity="left"/>

                    <TextView
                        android:id="@+id/baseproductTextView_222"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/kcchanbit"
                        android:textSize="10dp"
                        android:textColor="#B0B0B0"
                        android:gravity="left"/>
                </LinearLayout>

                <!-- Third LinearLayout -->
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="left">

                    <TextView
                        android:id="@+id/baseproductTextView_3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/kcchanbit"
                        android:gravity="left"/>

                    <TextView
                        android:id="@+id/baseproductTextView_33"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/kcchanbit"
                        android:gravity="left"
                        android:textColor="#483D8B"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/baseproductTextView_333"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/kcchanbit"
                        android:textSize="10dp"
                        android:textColor="#B0B0B0"
                        android:gravity="left"/>
                </LinearLayout>

            </LinearLayout>

            <TextView
                android:id="@+id/recommendLip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="16dp"
                android:fontFamily="@font/kcchanbit"
                android:text="추천 립"
                android:textColor="#FF07194B"
                android:textSize="16sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/lipimageView_1"
                    android:layout_width="130dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <ImageView
                    android:id="@+id/lipimageView_2"
                    android:layout_width="130dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center" />

                <ImageView
                    android:id="@+id/lipimageView_3"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_weight="1" />
            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginBottom="16dp"
                android:gravity="center_horizontal">

                <!-- First LinearLayout -->
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="left">

                    <TextView
                        android:id="@+id/lipproductTextView_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/kcchanbit"
                        android:gravity="left"/>

                    <TextView
                        android:id="@+id/lipproductTextView_11"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/kcchanbit"
                        android:gravity="left"
                        android:textColor="#483D8B"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/lipproductTextView_111"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/kcchanbit"
                        android:textSize="10dp"
                        android:textColor="#B0B0B0"
                        android:gravity="left"/>
                </LinearLayout>

                <!-- Second LinearLayout -->

                <!-- Third LinearLayout -->

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_gravity="left"
                    android:gravity="left"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/lipproductTextView_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/kcchanbit"
                        android:gravity="left" />

                    <TextView
                        android:id="@+id/lipproductTextView_22"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/kcchanbit"
                        android:gravity="left"
                        android:textColor="#483D8B"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/lipproductTextView_222"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/kcchanbit"
                        android:gravity="left"
                        android:textColor="#B0B0B0"
                        android:textSize="10dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="left">

                    <TextView
                        android:id="@+id/lipproductTextView_3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/kcchanbit"
                        android:gravity="left"/>

                    <TextView
                        android:id="@+id/lipproductTextView_33"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/kcchanbit"
                        android:textColor="#483D8B"
                        android:gravity="left"
                        android:textStyle="bold"
                        />

                    <TextView
                        android:id="@+id/lipproductTextView_333"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/kcchanbit"
                        android:textSize="10dp"
                        android:textColor="#B0B0B0"
                        android:gravity="left"/>
                </LinearLayout>


            </LinearLayout>
            <TextView
                android:id="@+id/recommendEye"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="16dp"
                android:fontFamily="@font/kcchanbit"
                android:text="추천 아이"
                android:textColor="#FF07194B"
                android:textSize="16sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/eyeimageView_1"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_weight="1" />

                <ImageView
                    android:id="@+id/eyeimageView_2"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_gravity="center" />

                <ImageView
                    android:id="@+id/eyeimageView_3"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_weight="1" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_marginBottom="16dp"
                android:gravity="center_horizontal">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="left">

                    <TextView
                        android:id="@+id/eyeproductTextView_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/kcchanbit"
                        android:gravity="left"/>

                    <TextView
                        android:id="@+id/eyeproductTextView_11"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/kcchanbit"
                        android:gravity="left"
                        android:textColor="#483D8B"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/eyeproductTextView_111"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/kcchanbit"
                        android:textSize="10dp"
                        android:textColor="#B0B0B0"
                        android:gravity="left"/>
                </LinearLayout>

                <!-- Second LinearLayout -->
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="left">

                    <TextView
                        android:id="@+id/eyeproductTextView_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/kcchanbit"
                        android:gravity="left" />

                    <TextView
                        android:id="@+id/eyeproductTextView_22"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/kcchanbit"
                        android:textColor="#483D8B"
                        android:gravity="left"
                        android:textStyle="bold"
                        />

                    <TextView
                        android:id="@+id/eyeproductTextView_222"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/kcchanbit"
                        android:textSize="10dp"
                        android:textColor="#B0B0B0"
                        android:gravity="left"/>
                </LinearLayout>

                <!-- Third LinearLayout -->
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="left">

                    <TextView
                        android:id="@+id/eyeproductTextView_3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/kcchanbit"
                        android:gravity="left"/>

                    <TextView
                        android:id="@+id/eyeproductTextView_33"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/kcchanbit"
                        android:textColor="#483D8B"
                        android:gravity="left"
                        android:textStyle="bold"
                        />

                    <TextView
                        android:id="@+id/eyeproductTextView_333"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/kcchanbit"
                        android:textSize="10dp"
                        android:textColor="#B0B0B0"
                        android:gravity="left"/>
                </LinearLayout>

            </LinearLayout>

            <Button
                android:id="@+id/button3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="20dp"
                android:fontFamily="@font/kcchanbit"
                android:text="로그아웃"
                android:textColor="#DDDDDD" />

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>